pipeline {
    agent any
    
    tools {
        maven 'maven'
    }
    
    stages {
        stage('Clone') {
            steps {
                git branch: 'main', credentialsId: 'your_credentials_id', url: 'https://github.com/neetaniraula/Boardgame.git'
            }
        }
        
      stage('maven compile ') {
          steps {
              sh "mvn compile"
          }
      }
      fail this step
        stage('Build') {
            steps {
                sh "mvn package"
            }
        }
    }
    
    
}
